{
  "version": 2,
  "buildCommand": "prisma generate && next build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "functions": {
    "app/**/*.{js,jsx,ts,tsx}": {
      "maxDuration": 30,
      "memory": 1024
    },
    "src/app/api/**/*.{js,ts}": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "env": {
    "PRISMA_SKIP_POSTINSTALL_GENERATE": "false"
  }
}
